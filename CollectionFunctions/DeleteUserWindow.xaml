<Window x:Class="CollectionFunctions.DeleteUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CollectionFunctions"
        mc:Ignorable="d"
        Title="DeleteUserWindow" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="FnameBox" HorizontalAlignment="Left" Height="23" Margin="46,70,0,0" TextWrapping="Wrap" Text="FName" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="SnameBox" HorizontalAlignment="Left" Height="23" Margin="46,109,0,0" TextWrapping="Wrap" Text="SName" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="SubmitButton" Content="Submit" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="69,137,0,0" Click="SubmitButton_Click"/>
        <TextBox x:Name="ErrorTextBox" HorizontalAlignment="Left" Height="48" Margin="10,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272" BorderBrush="White" IsReadOnly="True"/>

    </Grid>
</Window>
