<Window x:Class="CollectionFunctions.SerchUserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CollectionFunctions"
        mc:Ignorable="d"
        Title="SerchUserWindow" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="FnameBox" HorizontalAlignment="Left" Height="28" Margin="135,153,0,0" TextWrapping="Wrap" Text="FName" VerticalAlignment="Top" Width="142"/>
        <TextBox x:Name="SnameBox" HorizontalAlignment="Left" Height="28" Margin="135,201,0,0" TextWrapping="Wrap" Text="SName" VerticalAlignment="Top" Width="142"/>
        <Button x:Name="SubmitButton" Content="Submit" HorizontalAlignment="Left" VerticalAlignment="Top" Width="88" Margin="153,235,0,0" Click="SubmitButton_Click" Height="24"/>
        <TextBox x:Name="ErrorTextBox" HorizontalAlignment="Left" Height="148" Margin="10,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272" BorderBrush="White" IsReadOnly="True"/>

    </Grid>
</Window>
